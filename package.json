{
  "name": "fireball",
  "version": "0.5.0",
  "description": "Fireball is the Html5 game engine for the future.",
  "homepage": "http://fireball-x.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/fireball-x/fireball.git"
  },
  "author": "Firebox Technology",
  "license": "Firebox License",
  "bugs": {
    "url": "https://github.com/fireball-x/fireball/issues"
  },
  "scripts": {
    "preinstall": "node ./utils/init.js",
    "install": "gulp bootstrap && bower install && gulp npm-rebuild && gulp update",
    "test": "node ./utils/run-tests.js",
    "gendoc": "gulp cp-apisrc && firedoc build ./utils/api --theme notab && open apidocs/index.html"
  },
  "dependencies": {
    "async": "1.2.1",
    "browserify": "10.2.3",
    "chai": "2.3.0",
    "chalk": "1.0.0",
    "chokidar": "1.0.2",
    "commander": "2.8.1",
    "del": "1.2.0",
    "diff": "1.4.0",
    "event-stream": "3.3.1",
    "fire-fs": "0.2.1",
    "fire-path": "0.1.7",
    "fire-url": "0.1.6",
    "git-utils": "3.0.0",
    "globby": "2.0.0",
    "gulp": "3.8.11",
    "gulp-babel": "5.1.0",
    "gulp-less": "3.0.3",
    "gulp-sequence": "0.3.2",
    "gulp-sourcemaps": "1.5.2",
    "gulp-stylus": "2.0.3",
    "gulp-uglify": "1.2.0",
    "jsondiffpatch": "0.1.33",
    "lodash": "3.9.3",
    "lwip": "0.0.7",
    "minimatch": "2.0.8",
    "mocha": "2.2.5",
    "node-uuid": "1.4.2",
    "semver": "4.3.6",
    "sinon": "1.15.0",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.0.0",
    "winston": "1.0.0",
    "xmldom": "0.1.19"
  },
  "devDependencies": {
    "check-dependencies": "0.9.2",
    "gulp-changed": "1.2.1",
    "gulp-download-fire-shell": "0.1.3",
    "gulp-header": "1.2.2",
    "gulp-jshint": "1.11.0",
    "gulp-shell": "0.4.1",
    "gulp-watch": "4.2.4",
    "jshint-stylish": "2.0.0",
    "mkdirp": "0.5.1",
    "vulcanize": "1.8.1"
  },
  "builtins": [
    "assets",
    "canvas-assets",
    "console",
    "code-editor",
    "hierarchy",
    "inspector",
    "ipc-debugger",
    "package-manager",
    "scene",
    "tester",
    "timeline",
    "ui-gizmos",
    "ui-kit",
    "ui-property",
    "ui-tree",
    "fire-ui-kit",
    "color-picker",
    "pixi-grid"
  ],
  "runtimes": [
    "runtime-cocos2d-js",
    "runtime-pixi"
  ],
  "submodules": [
    "editor-framework",
    "engine-framework",
    "asset-db"
  ],
  "native-modules": [
    "chokidar",
    "git-utils",
    "lwip"
  ],
  "shared-packages": [
    "package-examples",
    "benchmark"
  ],
  "fire-shell-version": "0.27.3",
  "electron-version": "0.29.2",
  "main": "app.js",
  "firedoc": {
    "linkNatives": true,
    "attributesEmit": true,
    "path": "./utils/api",
    "dest": "./apidocs"
  }
